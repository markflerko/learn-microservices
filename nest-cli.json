{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mcrsers/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mcrsers/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/mcrsers",
  "projects": {
    "alarms-classifier-service": {
      "type": "application",
      "root": "apps/alarms-classifier-service",
      "entryFile": "main",
      "sourceRoot": "apps/alarms-classifier-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/alarms-classifier-service/tsconfig.app.json"
      }
    },
    "alarms-generator": {
      "type": "application",
      "root": "apps/alarms-generator",
      "entryFile": "main",
      "sourceRoot": "apps/alarms-generator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/alarms-generator/tsconfig.app.json"
      }
    },
    "alarms-service": {
      "type": "application",
      "root": "apps/alarms-service",
      "entryFile": "main",
      "sourceRoot": "apps/alarms-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/alarms-service/tsconfig.app.json"
      }
    },
    "mcrsers": {
      "type": "application",
      "root": "apps/mcrsers",
      "entryFile": "main",
      "sourceRoot": "apps/mcrsers/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mcrsers/tsconfig.app.json"
      }
    },
    "notification-service": {
      "type": "application",
      "root": "apps/notification-service",
      "entryFile": "main",
      "sourceRoot": "apps/notification-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-service/tsconfig.app.json"
      }
    },
    "workflow-service": {
      "type": "application",
      "root": "apps/workflow-service",
      "entryFile": "main",
      "sourceRoot": "apps/workflow-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/workflow-service/tsconfig.app.json"
      }
    },
    "workflows": {
      "type": "library",
      "root": "libs/workflows",
      "entryFile": "index",
      "sourceRoot": "libs/workflows/src",
      "compilerOptions": {
        "tsConfigPath": "libs/workflows/tsconfig.lib.json"
      }
    }
  }
}